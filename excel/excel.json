{

    "scope": "excel",
    "rules": [
        ["cell", "(<letter>_1- <digit>_1-)"],
        ["range", "<cell>_1-2"]
    ],

    "commands": [
        ["activate excel", "extensions.run(excel.main)"],
        ["select <range>", "{esc} extensions.call(excel.main, message, 'select range', $-1)"]
    ]

}