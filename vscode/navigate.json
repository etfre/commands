{
    "conditions": {
        "title": "Visual Studio Code"
    },
    "functions": [
        ["flystart()", "{ctrl, f1}"],
        ["flyend()", "{ctrl, f2}"],
        ["spaceBack()", "flystart() 'sb' flyend()"],
        ["spaceForward()", "flystart() 'sf' flyend()"],
        ["spaceAround(char)", "spaceBack() char spaceForward()"]
    ],
    "rules": [
        ["text_action", "(cut={ctrl, x} | copy={ctrl, c} | select='')"],
        ["motion", "(until=t | after=f | before=F | retreat=T)"],
        ["text_direction", "(left | right | down | up | final=end | first=home)"],
        ["textobject", "(word={ctrl, d} | file={ctrl, a} line={ctrl, i})"]
    ],
    "commands": [
        ["jump", "{shift, enter}"]
        ]
}