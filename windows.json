{
    "functions": [
    ],
    "rules": [
        [
            "chrome",
            "chrome='Google Chrome'"
        ],
        [
            "pillars",
            "pillars='Pillars of Eternity'"
        ],
        [
            "studio",
            "studio='microsoft visual studio'"
        ],
        [
            "code",
            "code='Visual Studio Code'"
        ],
        [
            "media",
            "media='vlc media player'"
        ],
        [
            "bash",
            "bash='evan@EVAN-PC'"
        ],
        ["terminal", "terminal='Terminal of Evan'"],
        ["cmdprompt", "prompt='command prompt'"],
        [
            "windows",
            "(<chrome> | <pillars> | <code> | <terminal> | <bash> | <media> | <studio> | <cmdprompt>)"
        ]
    ],
    "conditions": {},
    "commands": [
        ["banana hammock", "repeat({ctrl, a}, {esc}, 10)"],
        [
            "open <windows> <optional_number>",
            "window.focus($1, $2)"
        ],
        [
            "close window",
            "window.close()"
        ],
        ["minimize window", "window.minimize_active()"],
        ["maximize window", "window.maximize_active()"],
        [
            "start excel",
            "start('C:\\Program Files (x86)\\Microsoft VS Code\\Code.exe')"
        ],
        ["start terminal", "keys(ctrl, alt, t)"],
        ["start bash", "proc([path(r'C://Windows/System32/bash.exe')])"],
        ["start command prompt", "proc([path(r'C://Windows/System32/cmd.exe')])"],
        ["start studio", "start('C:\\Program Files (x86)\\Microsoft Visual Studio\\2017\\Community\\Common7\\IDE\\devenv.exe')"],
        ["start code", "proc(code)"],
        ["start chrome", "proc('start chrome')"],
        ["restart wifi", "thread(proc('netsh wlan connect MyWifi123'))"],
        ["switch to bash", "bash keys(enter)"]
    ]
}